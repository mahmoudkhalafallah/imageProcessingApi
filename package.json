  "main": "index.js",
  "scripts": {
    "start": "nodemon src/index.ts",
    "test": "npm run build && npm run jasmine",
    "jasmine": "jasmine",
    "prettier": "prettier -c .prettierrc.json 'src/**/*.ts' --write",
    "lint": "prettier && eslint --fix 'src/**/*.ts'",
    "build": "npm run lint && tsc -b"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mahmoudkhalafallah/imageProcessingApi.git"
  },
